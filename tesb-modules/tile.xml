<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<properties>
		<cxf.version>3.3.4</cxf.version>
		<camel.version>2.24.2</camel.version>
		<activemq.version>5.15.10</activemq.version>
		<karaf.version>4.2.7</karaf.version>
		<spring.version>5.1.9.RELEASE</spring.version>
	</properties>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>tesb-modules-echo-properties</id>
						<phase>generate-resources</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<tasks>
								<echo>tesb-modules: camel.version ${camel.version}</echo>
								<echo>tesb-modules: cxf.version ${cxf.version}</echo>
								<echo>tesb-modules: activemq.version ${activemq.version}</echo>
								<echo>tesb-modules: karaf.version ${karaf.version}</echo>
								<echo>tesb-modules: spring.version ${spring.version}</echo>
							</tasks>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

</project>
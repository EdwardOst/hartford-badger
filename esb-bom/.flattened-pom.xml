<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.talend.se.platform</groupId>
  <artifactId>esb-bom</artifactId>
  <version>7.3.1</version>
  <packaging>pom</packaging>
  <name>platform :: apache :: esb</name>
  <description>Platform pom that uses dependencyManagement imports of parent poms from component Apache projects to establish a consistent baseline set of dependency versions for alignment with the talend-esb platform pom.</description>
  <properties>
    <karaf.version>4.2.7</karaf.version>
    <cxf.version>3.3.4</cxf.version>
    <maven-compiler-plugin.version>3.8.1</maven-compiler-plugin.version>
    <spring.version>5.1.9.RELEASE</spring.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <activemq.version>5.15.10</activemq.version>
    <flatten-maven-plugin.version>1.2.1</flatten-maven-plugin.version>
    <tesb.version>7.3.1</tesb.version>
    <camel.version>2.24.2</camel.version>
    <revision>7.3.1</revision>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>com.talend.se.platform</groupId>
        <artifactId>cxf-bom</artifactId>
        <version>${project.version}</version>
        <type>pom</type>
        <classifier></classifier>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>com.talend.se.platform</groupId>
        <artifactId>camel-bom</artifactId>
        <version>${project.version}</version>
        <type>pom</type>
        <classifier></classifier>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>com.talend.se.platform</groupId>
        <artifactId>camel-bom</artifactId>
        <version>${project.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>com.talend.se.platform</groupId>
        <artifactId>karaf-bom</artifactId>
        <version>${project.version}</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
    </dependencies>
  </dependencyManagement>
</project>
